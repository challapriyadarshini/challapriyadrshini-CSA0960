public class main
{
  public static void main(String[] args) {
      int n1 =12,n2=18;
      int lcm=1;
      int gcd=1;
      for(int i=1;i<=n1&i<=n2;i++)
      {
          if(n1%i==0&&n2%i==0)
          {
              gcd=i;
              lcm=(n1*n2)/gcd;
          }
      }
      System.out.println("gcd ="+gcd);
      System.out.println("lcm ="+lcm);
  }
}
