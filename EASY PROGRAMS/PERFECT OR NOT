public class main
{
    public static void main(String[] args) {
        int n=6,temp;
        temp=n;
        int sum=0;
        for(int i=1;i<n;i++)
        {
            if(n%i==0)
            {
                sum=sum+i;
            }
        }
            if(sum==temp)
            {
                System.out.println("perfect");
            }
            else
            {
                System.out.println("not perfect");
            }
    }
} 
